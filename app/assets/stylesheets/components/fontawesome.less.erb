/* FontAwesome, only in non-test modes */
<% if (Jetlanding::Application.config.font_awesome) %>
@fontAwesomeEotPath:  '<%= asset_path 'font/fontawesome-webfont.eot' %>';
@fontAwesomeWoffPath: '<%= asset_path 'font/fontawesome-webfont.woff' %>';
@fontAwesomeTtfPath:  '<%= asset_path 'font/fontawesome-webfont.ttf' %>';
@fontAwesomeSvgzPath: '<%= asset_path 'font/fontawesome-webfont.svgz' %>';
@fontAwesomeSvgPath:  '<%= asset_path 'font/fontawesome-webfont.svg' %>';
@fontAwesomeEotPath_iefix: '<%= asset_path 'font/fontawesome-webfont.eot#iefix' %>';
@import "fontawesome";
<% end %>